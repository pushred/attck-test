{
  "name": "attck-code-test",
  "author": "Eric Lanehart <eric@pushred.co> (http://pushred.co)",
  "babel": {
    "presets": [
      "react"
    ]
  },
  "engines": {
    "node": ">=6.10.0",
    "yarn": ">=0.21.3"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "npm-run-all": "^4.0.2"
  },
  "scripts": {
    "dev": "npm-run-all --parallel build:* preview",
    "build:js": "babel --watch -o bundle.js index.js",
    "preview": "browser-sync start --server --files index.html index.css bundle.js --no-notify --no-open"
  }
}
